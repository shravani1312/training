import java.util.*;

public class Prime{
	public static int IsPrime(int inp){

		 for(int i=2;i<=inp/2;i++){
		        if(inp%i==0){
		            return 1;
		        }
		    }
		    return 2;

	}
	
public static void main(String[] args){
    Scanner st = new Scanner(System.in);
	int inp=st.nextInt();
     System.out.println(IsPrime(inp));
     st.close();
	
   }
}
