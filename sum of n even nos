import java.util.*;

public class Sum{
	public static int SumOfEvenDigits(int input1){
		int sum=0;
		int n=0;
		while(input1!=0){
			n=input1%10;
			if(n%2==0){
				sum+=n;
			}
			input1/=10;
		}
		return sum;
		}
	
public static void main(String[] args){
    Scanner st = new Scanner(System.in);
    int inp=st.nextInt();
    System.out.println(SumOfEvenDigits(inp));
    st.close();
	
   }
}
