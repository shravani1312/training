public Result findOriginalFirstndSum(int[]input1,int input2){
int sum=input1[input1.length-1];
for(int i=input1.length-2;i>=0;i--)
{
input[i]=input[i]-input[i+1];
sum+=input[i];
}
Result r=new Result(input[0]sum);
return r;
}
