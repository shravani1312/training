import java.io.*;
import java.util.*;
import java.lang.math.*;
class UserMainCode{
public int sumOfPowerOfDigits(intinput1){
integer sum=0,r=0,prev=0;
double f1,f2;
while(input1>0)
{
r=integer.valueOf(input1){
f1=double.valueOf(r);
f2=double.value(prev);
f1=Math.pow(f1,f2);
sum+=f1.intValue();
prev= integer.valueof(r);
input1/=10;
}
return sum;
}
}
